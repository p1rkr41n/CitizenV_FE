AD9B,EAEGM,IAFH,CAEQ,UAACC,MAAD,EAAY;AAChBL,MAAAA,GAAG,CAACK,MAAM,CAACC,IAAR,CAAH;AACD,KAJH,EAKGC,KALH,CAKS,UAACC,GAAD,EAAS;AACdP,MAAAA,GAAG,CAAC,yBAAD,CAAH;AACD,KAPH;AAQD,GATM,CAAP;AAUD;AAED,OAAO,SAASS,WAAT,GAAuB;AAC5B,MAAMC,IAAI,GAAGC,YAAY,CAACC,OAAb,CAAqB,MAArB,CAAb;;AAEA,MAAI,CAACF,IAAL,EAAW;AACT,WAAO,IAAP;AACD;;AAED,SAAOG,IAAI,CAACC,KAAL,CAAWJ,IAAX,CAAP;AACD","sourcesContent":["export function login(credential) {\n  return new Promise((res, rej) => {\n    axios\n      .post(\"/api/auth/login\", credential)\n      .then((result) => {\n        res(result.data);\n      })\n      .catch((err) => {\n        rej(\"Wrong email or password\");\n      });\n  });\n}\n\nexport function register(credential) {\n  return new Promise((res, rej) => {\n    axios\n      .post(\"/api/auth/register\", credential)\n      .then((result) => {\n        res(result.data);\n      })\n      .catch((err) => {\n        rej(\"Wrong email or password\");\n      });\n  });\n}\n\nexport function currentUser() {\n  const user = localStorage.getItem(\"user\");\n\n  if (!user) {\n    return null;\n  }\n\n  return JSON.parse(user);\n}\n"]}]}